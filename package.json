{
  "name": "@ximu/notice-bar",
  "version": "1.0.1",
  "homepage": "https://TheSunset.github.io/notice-bar",
  "description": "a react component of notice bar",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "files": [
    "demo",
    "package.json",
    "dist"
  ],
  "keywords": [
    "react",
    "components",
    "notice-bar"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build:demo": "webpack --mode production --progress",
    "build:tsc-cjs": "tsc -b tsconfig.cjs.json",
    "build:tsc-esm": "tsc -b tsconfig.esm.json",
    "build:tsc": "tsc",
    "compile:sass": "sass src/:lib/cjs/",
    "clean": "rm -rf lib && mkdir lib",
    "babel:compile": "yarn run clean && babel src --out-dir lib --copy-files",
    "build:rollup": "rm -rf dist && mkdir dist && rollup -c",
    "deploy": "gh-pages -d demo"
  },
